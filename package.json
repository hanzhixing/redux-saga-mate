{
  "name": "redux-saga-mate",
  "version": "1.1.0",
  "description": "Allow you building react and redux based web apps with less pain.",
  "keywords": [
    "react",
    "react-redux",
    "redux",
    "redux-saga"
  ],
  "homepage": "https://hanzhixing.github.io/redux-saga-mate/",
  "repository": {
    "type": "git",
